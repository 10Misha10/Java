        Scanner sc = new Scanner(System.in);
        int n;
        while(!(sc.hasNextInt())){
            System.out.println("Некорректный ввод!");
            sc.nextLine();
        }
        n = sc.nextInt();
        for(int i = 0; i < 4; i++){
            System.out.println(Math.pow(n, i));
        }
